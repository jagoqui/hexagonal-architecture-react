{
  "files": [],
  // "references": [
  //   // { "path": "./tsconfig.app.json" },
  //   { "path": "./tsconfig.node.json" }
  // ],
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/data/*": ["data/*"],
      "@/*": ["src/app/*"]
    },
    "allowJs": true,
		"esModuleInterop": true,
		"jsx": "react-jsx",
		"module": "es6",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"outDir": "./dist/",
		"resolveJsonModule": true,
		"sourceMap": true,
		"strict": true,
		"target": "es2015"
  },
  "include": ["vite.config.ts"]
}
